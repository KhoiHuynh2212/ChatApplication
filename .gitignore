cat > .gitignore << 'EOF'
# IDE files
.idea/
*.iml

# Server (Java/Maven)
server/target/
server/*.jar
server/*.war
server/*.ear
server/.settings/
server/.project
server/.classpath

# Client files (adjust based on your client technology)
client/node_modules/
client/dist/
client/build/
client/.env.local
client/.env.development.local
client/.env.test.local
client/.env.production.local

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS files
.DS_Store
Thumbs.db
EOF
